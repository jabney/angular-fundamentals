{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "member-access": false,
        "no-conditional-assignment": false,
        "no-console": false,
        "no-empty": false,
        "no-string-literal": false,
        "only-arrow-functions": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "quotemark": {
            "options": ["single"],
            "severity": "warn"
        },
        "semicolon": {
            "options": ["never"],
            "severity": "warn"
        },
        "trailing-comma": false,
        "variable-name": false
    },
    "rulesDirectory": []
}
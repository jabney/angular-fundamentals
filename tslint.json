{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "quotemark": {
            "options": ["single"],
            "severity": "warn"
        },
        "semicolon": {
            "options": ["never"],
            "severity": "warn"
        },
        "trailing-comma": false
    },
    "rulesDirectory": []
}